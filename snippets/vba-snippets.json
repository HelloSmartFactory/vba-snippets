    Boolean
    Integer
    Long	
    Single	
    Double 
    Currency	
    Date	
    String
    Object	
    Variant
    */

    "Boolean": {
        "prefix": "Dimb",
        "body": [
            "Dim bol${1:varName} As Boolean"
        ],
        "description": "Boolean declaration"
